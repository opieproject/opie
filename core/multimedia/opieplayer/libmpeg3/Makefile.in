		video/idct.c \
		video/macroblocks.c \
		video/mmxtest.c \
		video/motion.c \
		video/mpeg3video.c \
		video/output.c \
		video/reconstruct.c \
		video/seek.c \
		video/slice.c \
		video/vlc.c
OBJECTS =	libmpeg3plugin.o \
		libmpeg3pluginimpl.o \
		bitstream.o \
		libmpeg3.o \
		mpeg3atrack.o \
		mpeg3css.o \
