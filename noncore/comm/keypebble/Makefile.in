kvnc.o: kvnc.cpp \
		$(QPEDIR)/include/qpe/qpemenubar.h \
		$(QPEDIR)/include/qpe/qpetoolbar.h \
		$(QPEDIR)/include/qpe/qpeapplication.h \
		$(QPEDIR)/include/qpe/global.h \
